{
  "Dir": {
    "data_root": "data/GENEA/processed",
    "log_root": "results/GENEA/fix_training"
  },
  "Glow": {
    "hidden_channels": 512,
    "K": 16,
    "actnorm_scale": 1.0,
    "flow_permutation": "invconv",
    "flow_coupling": "affine",
    "network_model": "LSTM",
    "num_layers": 2,
    "LU_decomposed": true,
    "distribution": "normal"
  },
  "Data": {
    "framerate": 20,
    "seqlen": 5,
    "n_lookahead": 20,
    "dropout": 0.4
  },
  "Optim": {
    "name": "adam",
    "args": {
      "lr": 0.0009,
      "betas": [
        0.9,
        0.999
      ],
      "eps": 1e-08
    },
    "Schedule": {
      "name": "default",
      "args": {
      }
    }
  },
  "Device": {
    "glow": [
      "cuda:0"
    ],
    "data": "cuda:0"
  },
  "Train": {
    "batch_size": 10,
    "num_batches": 10000,
    "max_grad_clip": 0,
    "max_grad_norm": 0,
    "max_checkpoints": 1,
    "checkpoints_gap": 2500,
    "num_plot_samples": 4,
    "scalar_log_gap": 50,
    "validation_log_gap": 500,
    "plot_gap": 2500,
    "warm_start": ""
  },
  "Temp": {
    "use_train_subset": false,
    "subset_size": 10
  },
  
  "Infer": {
    "pre_trained": ""
  }
}
